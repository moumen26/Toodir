{
  "expo": {
    "name": "TooDir",
    "slug": "TooDir",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.zynoxi12.TooDir"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/index.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/index.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "https://toodir-server-side.onrender.com/api",
      "filesUrl": "https://toodir-server-side.onrender.com",
      "router": {},
      "eas": {
        "projectId": "f4aa15d2-37bb-448f-9de4-8420f8fdb313"
      }
    }
  }
}
